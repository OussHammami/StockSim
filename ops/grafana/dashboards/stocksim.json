{
  "uid": "stocksim",
  "title": "StockSim Overview",
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "5s",
  "time": { "from": "now-15m", "to": "now" },
  "panels": [
    {
      "id": 1,
      "type": "timeseries",
      "title": ".NET GC alloc rate",
      "datasource": { "type": "prometheus", "uid": "prom" },
      "gridPos": { "h": 9, "w": 12, "x": 0, "y": 0 },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(process_runtime_dotnet_gc_allocations_size_bytes_total[1m])) by (exported_job)",
          "legendFormat": "{{exported_job}}"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "bytes/sec" }, "overrides": [] },
      "options": { "legend": { "displayMode": "list", "placement": "bottom" } }
    },
    {
      "id": 2,
      "type": "timeseries",
      "title": "HTTP requests/sec",
      "datasource": { "type": "prometheus", "uid": "prom" },
      "gridPos": { "h": 9, "w": 12, "x": 12, "y": 0 },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(http_server_request_duration_seconds_count[1m])) by (exported_job)",
          "legendFormat": "{{exported_job}}"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "req/s" }, "overrides": [] },
      "options": { "legend": { "displayMode": "list", "placement": "bottom" } }
    }
  ],
  "templating": { "list": [] }
}
