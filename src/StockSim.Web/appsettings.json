{
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=StockSim.Web.db"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "MarketFeed": {
    "BaseUrl": "http://localhost:8081"
  },
  "Rabbit": {
    "Host": "localhost",
    "Port": 5672,
    "User": "stocksim",
    "Pass": "stocksim",
    "Queue": "stocksim.events",
    "Durable": true
  },
  "Cors": {
    "AllowedOrigins": [
      "http://localhost:8080",
      "http://localhost:5173",
      "http://localhost:8081"
    ]
  }
}
